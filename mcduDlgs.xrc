                    <object class="sizeritem">
                        <flag>wxALIGN_CENTER_VERTICAL</flag>
                        <object class="wxChoice" name="ID_COMBO_CONNECTION_TYPE">
                            <size>120,-1</size>
                            <content>
                                <item>UDP</item>
                                <item>DDC</item>
                                <item>TechSat</item>
                                <item>(Simulated)</item>
                            </content>
                        </object>
                    </object>
                </object>
