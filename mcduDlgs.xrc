            <orient>wxHORIZONTAL</orient>

          <!-- MCDU Number: [ID_COMBO_MCDU] combobox -->

          <object class="sizeritem">
            <flag>wxALIGN_CENTER_VERTICAL|wxALIGN_RIGHT</flag>
            <object class="wxBoxSizer">
              <orient>wxHORIZONTAL</orient>
              <object class="sizeritem">
                <flag>wxALIGN_CENTER_VERTICAL</flag>
                <object class="wxStaticText" name="wxID_STATIC">
                  <font>
                    <weight>bold</weight>
                  </font>
                  <label>MCDU Number: </label>
                </object>
              </object>
              <object class="sizeritem">
                <flag>wxALIGN_CENTER_VERTICAL</flag>
                <object class="wxChoice" name="ID_COMBO_MCDU">
                  <size>120,-1</size>
                  <content>
                    <item>1</item>
                    <item>2</item>
                    <item>3</item>
                    <item>4</item>
                  </content>
                </object>
              </object>
            </object>
          </object>

          <!-- MCDU Number: [ID_COMBO_APPEARANCE] combobox -->

