                <flag>wxALIGN_CENTER_VERTICAL</flag>
                <object class="wxChoice" name="ID_COMBO_APPEARANCE">
                  <size>120,-1</size>
                  <content>
                    <item>Standard (14 lines)</item>
                    <item>TCP (10 lines)</item>
                  </content>
                </object>
              </object>
            </object>
